{
    "manifest_version": 3,
    "name": "SparkZ",
    "version": "1.0",
    "description": "A personalized tab page with useful widgets.",
    "chrome_url_overrides": {
        "newtab": "newtab.html"
    },
    "permissions": [
        "storage"
    ],
    "icons": {
        "16": "icons/favicon-16.png",
        "48": "icons/favicon-48.png",
        "128": "icons/favicon-128.png"
    }
}